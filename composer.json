{
    "name": "gravatarphp/gravatar-bundle",
    "type": "library",
    "description": "A bundle to integrate Gravatar into symfony",
    "keywords": [
        "gravatarphp",
        "gravatar-bundle",
        "bundle"
    ],
    "homepage": "https://github.com/gravatarphp/gravatar-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "George Mponos",
            "email": "gmponos@gmail.com",
            "homepage": "https://github.com/gravatarphp",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "gravatarphp/gravatar": "^2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^8.5.6 || ^9.2.2",
        "squizlabs/php_codesniffer": "^3.5.5"
    },
    "autoload": {
        "psr-4": {
            "Gravatar\\GravatarBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gravatar\\GravatarBundle\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
